{
    "plugins": ["plugins/markdown"],
    "source": {
				"include": [ "gauss.js" ],
        "exclude": [],
        "includePattern": "js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
			"referenceTitle": "HURL Documentation",
			"disableSort": true,
			"collapse": true,
			"resources": {
				"@thi.ng/umbrella": "http://thi.ng/umbrella"
			}
    },
		"opts": {
				"template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
				"sort": false,
				"theme_opts": {
            "theme": "dark",
						"title": "gauss-js",
				    "menu": [{
								"title": "Github",
								"link": "https://github.com/Marcos30004347/gauss"
						}]
        }
    }
}
